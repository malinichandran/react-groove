{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "abc123",
            "first_name": "",
            "last_name": "",
            "email": "",
            "password": " ",
            "profile_pic": " ",
            "country": " "
        }
    ],
    "required": [
        "username",
        "first_name",
        "last_name",
        "email",
        "password"
        
    ],
    "properties": {
        "username": {
            "maxLength": 25,
            "minLength": 6,
            "type": "string"
        },
        "first_name": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
        },
        "last_name": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "password": {
            "maxLength": 25,
            "minLength": 6,
            "type": "string"
        },
        "profile_pic": {
            "type": "string"
        },
        "country": {
            "type": "string"
        }
    },
    "additionalProperties": true
}